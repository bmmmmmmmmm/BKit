  "scripts": {
    "build": "tsc",
    "clear:dist": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "rebuild": "npm run clear:dist && npm run build",
    "version:patch": "npm version patch --no-git-tag-version",
    "public": "npm run rebuild && npm run version:patch && npm publish --access public"
  },
  "files": [
    "dist/**/*.js"
  ],
  "bin": {
    "bk": "./dist/bin/main.js",
    "rb": "./dist/bin/rb.js",
    "rbl": "./dist/bin/rbl.js",
    "le": "./dist/bin/le.js"
  }
}
